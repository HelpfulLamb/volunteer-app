    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": ["html", "text", "json"],
    "collectCoverageFrom": [
      "controllers/**/*.js",
      "models/notificationModel.js",
      "models/itemModel.js",
