      "utils/**/*.js",
      "services/**/*.js",
      "!controllers/volunteerHistoryController.js"
    ],
    "coverageDirectory": "coverage",
    "testTimeout": 10000,
    "forceExit": true,
    "detectOpenHandles": true,
    "testEnvironment": "node"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.17",
    "bcrypt": "^6.0.0",
