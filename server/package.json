    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mysql2": "^3.14.2",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.17.1",
    "tslib": "^2.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3"
  }
